<%# Add a blank line before this block %>
<Input <%= @name %>>
  Module      <%= @input_module %>
  <%- if @input_query -%>
  Query       <%= @input_query %>
  <%- end -%>
  <%- if @input_execs -%>
    <%- @input_execs.each do |exec| -%>
  Exec        <%= "#{exec};" %>
    <%- end -%>
  <%- end -%>
  <%- if @input_file_path -%>
  File        <%= "'#{@input_file_path}'" %>
  <%- end -%>
</Input>
